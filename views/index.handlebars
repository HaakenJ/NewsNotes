<div class="container">
  <div id="article-cards">
    {{#each articles}}
    {{> articles/article-block}}
    {{/each}}
  </div>
</div>

<div id="notes"></div>

<div id="modal1" class="modal">
  <div class="modal-content">
    <h4 id="modal-title"></h4>
    <input placeholder="Name of note" id="titleinput" type="text" name="title" class="validate">
    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="input-field col s12">
            <textarea id="bodyinput" name="body" class="materialize-textarea"></textarea>
          </div>
        </div>
      </form>
    </div>
  </div>
  <div class="modal-footer">
    <a href="#!" id="savenote" class="modal-close waves-effect waves-green btn-flat" data-id="">Save Note</a>
  </div>
</div>`;